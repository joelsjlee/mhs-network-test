            <!-- M/CFA/15.1 — JQA’s Senate Diary -->

            <div type="month" mhs:startingPage="1" mhs:precedingFile="JQADiaries-v51-1803-02-p001.xml" mhs:followingFile="JQADiaries-v51-1803-07-p001.xml"
