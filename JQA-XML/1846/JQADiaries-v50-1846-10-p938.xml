					<bibl><author>JQA</author><date type="creation" when="1846-10-25"/><editor
							role="transcription">Neal Millikan</editor></bibl>
					<div type="docbody">
						<p><date>25. </date>Emmons M<hi rend="superscript">r</hi> and M<hi rend="superscript"
								>rs.</hi></p>
						<p>Christian Psalter Hymn 8</p>
					</div>
				</div>
			
				
				<div type="entry" xml:id="jqadiaries-v50-1846-10-26">
					<head>26 October 1846</head>
